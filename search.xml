  { name: "清气化痰丸", category: "清热化痰类" },            { name: "橘红丸", category: "清热化痰类" },            { name: "半夏天麻丸", category: "化痰息风类" },            { name: "消瘿丸", category: "化痰散结类" },            // 第十二章 止咳平喘中成药            { name: "通宣理肺丸", category: "止咳类" },            { name: "杏苏止咳颗粒", category: "止咳类" },            { name: "清肺抑火丸", category: "止咳类" },            { name: "蛇胆川贝液", category: "止咳类" },            { name: "急支糖浆", category: "止咳类" },            { name: "养阴清肺膏", category: "止咳类" },            { name: "二母宁嗽丸", category: "止咳类" },            { name: "小青龙颗粒", category: "平喘类" },            { name: "止嗽定喘口服液", category: "平喘类" },            { name: "降气定喘丸", category: "平喘类" },            { name: "人参保肺丸", category: "平喘类" },            { name: "苏子降气丸", category: "平喘类" },            { name: "七味都气丸", category: "平喘类" },            { name: "固本咳喘片", category: "平喘类" },            // 第十三章 开窍中成药            { name: "安宫牛黄丸", category: "凉开类" },            { name: "紫雪散", category: "凉开类" },            { name: "局方至宝散", category: "凉开类" },            { name: "清开灵注射液", category: "凉开类" },            { name: "苏合香丸", category: "温开类" },            // 第十四章 补益中成药            { name: "四君子丸", category: "补气类" },            { name: "补中益气丸", category: "补气类" },            { name: "参苓白术丸", category: "补气类" },            { name: "启脾丸", category: "补气类" },            { name: "桂附地黄丸", category: "补阳类" },            { name: "右归丸", category: "补阳类" },            { name: "济生肾气丸", category: "补阳类" },            { name: "五子衍宗丸", category: "补阳类" },            { name: "当归补血口服液", category: "补血类" },            { name: "四物合剂", category: "补血类" },            { name: "六味地黄丸", category: "补阴类" },            { name: "左归丸", category: "补阴类" },            { name: "河车大造胶囊", category: "补阴类" },            { name: "知柏地黄丸", category: "补阴类" },            { name: "八珍颗粒", category: "气血双补类" },            { name: "归脾丸", category: "气血双补类" },            { name: "十全大补丸", category: "气血双补类" },            { name: "人参养荣丸", category: "气血双补类" },  